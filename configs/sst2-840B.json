{
  "task": "classify",
  "batchsz": 50,
  "basedir": "./sst2-lstm",
  "features": [
    {
      "name": "word",
      "vectorizer": {
        "type": "token1d"
      },
      "embeddings": {
        "label": "glove-840B"
      }
    }
  ],
  "preproc": {
    "clean": true
  },
  "backend": "tensorflow",
  "dataset": "SST2",
  "loader": {
    "reader_type": "default"
  },
  "unif": 0.25,
  "model": {
    "model_type": "lstm",
    "hsz": 400,
    "dropout": 0.5
  },
  "train": {
    "epochs": 5,
    "optim": "adamw",
    "eta": 0.0008,
    "weight_decay": 1e-05,
    "early_stopping_metric": "acc"
  }
}